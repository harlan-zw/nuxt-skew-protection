{
  "name": "nuxt-skew-protection",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.16.1",
  "description": "Zero-config real-time notifications and long-lived build assets to prevent skew issues in Nuxt applications.",
  "author": {
    "name": "Harlan Wilton",
    "email": "harlan@harlanzw.com",
    "url": "https://harlanzw.com/"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://nuxtseo.com/skew-protection",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-skew-protection.git"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-skew-protection/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./server": "./dist/runtime/server/index.js"
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --fix",
    "build": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt-module-build build && nuxi prepare playground",
    "release": "pnpm build && bumpp -x \"npx changelogen --output=CHANGELOG.md\"",
    "prepack": "pnpm run build",
    "test": "vitest",
    "test:unit": "vitest run test/unit",
    "test:integration": "vitest run test/integration",
    "test:e2e": "vitest run test/e2e",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:watch": "vitest",
    "test:cloudflare": "node --experimental-strip-types scripts/test-cloudflare.ts",
    "test:node": "node --experimental-strip-types scripts/test-node.ts",
    "test:static": "node --experimental-strip-types scripts/test-static.ts",
    "test:sse": "node --experimental-strip-types scripts/test-sse.ts",
    "typecheck": "nuxt typecheck",
    "test:attw": "attw --pack"
  },
  "dependencies": {
    "@nuxt/kit": "^4.1.2",
    "@nuxtjs/robots": "^5.5.5",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "nuxt-site-config": "^3.2.9",
    "pathe": "^2.0.3",
    "pkg-types": "^2.3.0",
    "std-env": "^3.9.0",
    "ufo": "^1.6.1",
    "unstorage": "^1.15.1"
  },
  "optionalDependencies": {
    "cloudflare": "^3.7.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@arethetypeswrong/cli": "^0.18.2",
    "@headlessui/vue": "^1.7.23",
    "@iconify-json/carbon": "^1.2.13",
    "@iconify-json/logos": "^1.2.9",
    "@iconify-json/ri": "^1.2.5",
    "@iconify-json/tabler": "^1.2.22",
    "@nuxt/content": "^3.7.1",
    "@nuxt/devtools-ui-kit": "^2.6.3",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "^10.1.0",
    "@unocss/nuxt": "^66.5.1",
    "@unocss/preset-icons": "^66.5.1",
    "@unocss/preset-uno": "^66.5.1",
    "@unocss/runtime": "^66.5.1",
    "@vueuse/nuxt": "^13.9.0",
    "better-sqlite3": "^12.2.0",
    "bumpp": "^10.2.3",
    "eslint": "^9.35.0",
    "eventsource": "^2.0.2",
    "execa": "^9.6.0",
    "firebase-functions": "^6.4.0",
    "happy-dom": "^18.0.1",
    "nuxt": "^4.1.2",
    "playwright-core": "^1.55.0",
    "typescript": "^5.9.2",
    "unocss": "^66.5.1",
    "vitest": "^3.2.4",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.1.0",
    "wrangler": "^4.40.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@firebase/util",
      "@parcel/watcher",
      "better-sqlite3",
      "esbuild",
      "protobufjs",
      "unrs-resolver",
      "vue-demi"
    ]
  },
  "resolutions": {
    "nuxt-skew-protection": "workspace:*"
  }
}
