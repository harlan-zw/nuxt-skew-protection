{
  "name": "nuxt-skew-protection",
  "type": "module",
  "version": "0.1.3",
  "packageManager": "pnpm@10.18.0",
  "description": "Instantly adopted Nuxt releases with long-lived build assets.",
  "author": {
    "name": "Harlan Wilton",
    "email": "harlan@harlanzw.com",
    "url": "https://harlanzw.com/"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://nuxtseo.com/skew-protection",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-skew-protection.git"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-skew-protection/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./server": "./dist/runtime/server/index.js"
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --fix",
    "build": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt-module-build build && nuxi prepare playground",
    "release": "pnpm build && bumpp -x \"npx changelogen --output=CHANGELOG.md\"",
    "prepack": "pnpm run build",
    "test": "vitest",
    "test:unit": "vitest run test/unit",
    "test:integration": "vitest run test/integration",
    "test:e2e": "vitest run test/e2e",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:watch": "vitest",
    "test:cloudflare-polling": "node --experimental-strip-types scripts/test-cloudflare-polling.ts",
    "test:node-sse": "node --experimental-strip-types scripts/test-node-sse.ts",
    "test:node-polling": "node --experimental-strip-types scripts/test-node-polling.ts",
    "test:node-ws": "node --experimental-strip-types scripts/test-node-ws.ts",
    "test:static-polling": "node --experimental-strip-types scripts/test-static-polling.ts",
    "test:vercel-sse": "node --experimental-strip-types scripts/test-vercel-sse.ts",
    "typecheck": "nuxt typecheck",
    "test:attw": "attw --pack"
  },
  "dependencies": {
    "@nuxt/kit": "^4.1.3",
    "@nuxtjs/robots": "^5.5.5",
    "consola": "^3.4.2",
    "cookie-es": "^2.0.0",
    "defu": "^6.1.4",
    "nuxt-site-config": "^3.2.9",
    "pathe": "^2.0.3",
    "pkg-types": "^2.3.0",
    "std-env": "^3.9.0",
    "tinyexec": "^1.0.1",
    "ufo": "^1.6.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@arethetypeswrong/cli": "^0.18.2",
    "@nuxt/content": "^3.7.1",
    "@nuxt/devtools-ui-kit": "^2.6.5",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxt/ui": "^4.0.1",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "^10.1.0",
    "@types/ioredis": "^5.0.0",
    "@types/ws": "^8.18.1",
    "@unocss/nuxt": "^66.5.2",
    "@unocss/preset-icons": "^66.5.2",
    "@unocss/preset-uno": "^66.5.2",
    "@unocss/runtime": "^66.5.2",
    "@vueuse/nuxt": "^13.9.0",
    "better-sqlite3": "^12.4.1",
    "bumpp": "^10.3.1",
    "eslint": "^9.37.0",
    "eventsource": "^4.0.0",
    "execa": "^9.6.0",
    "firebase-functions": "^6.5.0",
    "happy-dom": "^19.0.2",
    "ioredis": "^5.8.1",
    "nuxt": "^4.1.3",
    "playwright-core": "^1.56.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "unocss": "^66.5.2",
    "unstorage": "^1.17.1",
    "vitest": "^3.2.4",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.1.1",
    "wrangler": "^4.42.1",
    "ws": "^8.18.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@firebase/util",
      "@parcel/watcher",
      "better-sqlite3",
      "esbuild",
      "protobufjs",
      "unrs-resolver",
      "vue-demi"
    ]
  },
  "resolutions": {
    "nuxt-skew-protection": "workspace:*"
  }
}
